# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.log

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/spec/examples.txt

# Rubymine
/.idea

# rbenv local file
.ruby-version

# Ignore test coverage
/coverage

# Emacs backup files
*~

# Folder attributes file on Macs
.DS_Store

# Configuration files
Gemfile.local

# Ignore capistrano
/Capfile
/config/deploy.rb
/config/deploy/
/lib/capistrano

# Generated docs
/.yardoc/
/doc/

# vim
tags
*.swp
*.swo

# dev. repository
/incoming
/repo

# local config
config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml
